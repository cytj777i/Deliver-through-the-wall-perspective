--è¿™ä¸‹é¢å¡«åŠ è½½å¤–éƒ¨è„šæœ¬
local CONFIG = {
    API_URL = "https://robloxserver2.tagonmiku.workers.dev",
    MAP_DATA = {
        name = "è‡ªç„¶ç¾å®³",
        mapId = "natural_disaster", 
        requireKey = true
    }
}

-- æœåŠ¡
local Players = game:GetService("Players")
local HttpService = game:GetService("HttpService")

-- æ‚¨æä¾›çš„å¯†é’¥
local NATURAL_DISASTER_KEY = "6bce9a543936bae2c6038a6585b8a948_NDS"

-- è¾…åŠ©å‡½æ•°
local function urlEncode(text)
    if not text then return "" end
    text = string.gsub(text, "\n", "\r\n")
    text = string.gsub(text, "([^%w _%%%-%.~])", function(c)
        return string.format("%%%02X", string.byte(c))
    end)
    return string.gsub(text, " ", "+")
end

local function extractJson(text)
    if not text then return "" end
    local startPos = string.find(text, "{")
    local jsonText
    if startPos then
        jsonText = string.sub(text, startPos)
        if not jsonText then jsonText = text end
    else
        jsonText = text
    end
    return jsonText
end

local function showNotification(message)
    local PlayerGui = Players.LocalPlayer:WaitForChild("PlayerGui")
    
    -- åˆ›å»ºé€šçŸ¥GUI
    local notification = Instance.new("ScreenGui")
    notification.Name = "NDLauncherNotification"
    notification.ResetOnSpawn = false
    notification.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    notification.Parent = PlayerGui

    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(0, 300, 0, 60)
    frame.Position = UDim2.new(0.5, -150, 0, 20)
    frame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
    frame.BorderSizePixel = 0
    frame.Parent = notification
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 8)
    corner.Parent = frame
    
    local stroke = Instance.new("UIStroke")
    stroke.Color = Color3.fromRGB(0, 255, 0)
    stroke.Thickness = 2
    stroke.Parent = frame
    
    local label = Instance.new("TextLabel")
    label.Size = UDim2.new(1, -20, 1, 0)
    label.Position = UDim2.new(0, 10, 0, 0)
    label.BackgroundTransparency = 1
    label.Text = message
    label.TextColor3 = Color3.fromRGB(255, 255, 255)
    label.TextSize = 14
    label.Font = Enum.Font.Gotham
    label.TextWrapped = true
    label.Parent = frame
    
    -- 4ç§’åè‡ªåŠ¨ç§»é™¤
    task.delay(4, function()
        notification:Destroy()
    end)
end

-- ä¸»è¦åŠ è½½å‡½æ•°
local function loadNaturalDisasterScript()
    showNotification("ğŸŒªï¸ æ­£åœ¨å¯åŠ¨è‡ªç„¶ç¾å®³è„šæœ¬...")
    
    -- éªŒè¯å¯†é’¥æ ¼å¼
    if not NATURAL_DISASTER_KEY or NATURAL_DISASTER_KEY == "" then
        showNotification("âŒ é”™è¯¯ï¼šæœªæä¾›å¯†é’¥")
        return false
    end
    
    -- æ„å»ºéªŒè¯URL
    local hwid = tostring(game:GetService("RbxAnalyticsService"):GetClientId())
    local checksum = HttpService:GenerateGUID(false)
    
    local url = string.format("%s/check_key?key=%s&hwid=%s&map_id=%s&checksum=%s", 
        CONFIG.API_URL, 
        urlEncode(NATURAL_DISASTER_KEY), 
        urlEncode(hwid), 
        urlEncode(CONFIG.MAP_DATA.mapId), 
        urlEncode(checksum))
    
    showNotification("ğŸ”‘ æ­£åœ¨éªŒè¯å¯†é’¥...")
    
    -- å‘é€APIè¯·æ±‚
    local success, response = pcall(function()
        return game:HttpGet(url)
    end)
    
    if not success then
        showNotification("âŒ è¿æ¥å¤±è´¥ - è¯·æ£€æŸ¥ç½‘ç»œè¿æ¥")
        return false
    end
    
    -- è§£æå“åº”
    local jsonText = extractJson(response)
    local decodeSuccess, result = pcall(function()
        return HttpService:JSONDecode(jsonText)
    end)
    
    if not decodeSuccess then
        showNotification("âŒ æœåŠ¡å™¨å“åº”æ— æ•ˆ")
        return false
    end
    
    -- æ£€æŸ¥å¯†é’¥æ˜¯å¦æœ‰æ•ˆ
    if result.status == "success" and result.data and result.data.script then
        showNotification("âœ… å¯†é’¥éªŒè¯æˆåŠŸï¼æ­£åœ¨åŠ è½½è„šæœ¬...")
        
        -- ä¸ºè„šæœ¬è®¾ç½®å…¨å±€å˜é‡
        _G.Key = NATURAL_DISASTER_KEY
        _G.MapID = CONFIG.MAP_DATA.mapId
        _G.KeyInfo = result.data
        
        -- æ‰§è¡Œè„šæœ¬
        task.wait(1)
        local execSuccess, errorMsg = pcall(function()
            loadstring(result.data.script)()
        end)
        
        if execSuccess then
            showNotification("ğŸ® è‡ªç„¶ç¾å®³è„šæœ¬å·²æ¿€æ´»ï¼")
            return true
        else
            showNotification("âŒ è„šæœ¬æ‰§è¡Œå¤±è´¥")
            warn("è‡ªç„¶ç¾å®³è„šæœ¬é”™è¯¯:", errorMsg)
            return false
        end
    else
        showNotification("âŒ å¯†é’¥æ— æ•ˆ: " .. (result.message or "æœªçŸ¥é”™è¯¯"))
        return false
    end
end

-- è‡ªåŠ¨æ‰§è¡Œå¹¶å¤„ç†é”™è¯¯
local function initialize()
    local success, err = pcall(function()
        loadNaturalDisasterScript()
    end)
    
    if not success then
        showNotification("âŒ å¯åŠ¨å™¨é”™è¯¯: " .. tostring(err))
    end
end

-- å¯åŠ¨è¿‡ç¨‹
initialize()



end)

if not success then
    warn("åŠ è½½å¤±è´¥:", err)
end