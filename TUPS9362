-- 由XTTT使用91Ai制作
local Players = game:GetService("Players")
local Lighting = game:GetService("Lighting")
local RunService = game:GetService("RunService")
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")

-- 只对特定用户名的玩家创建UI
local TARGET_USERNAME = "iiiii11111iiiii6"

-- 在服务器端为特定玩家创建UI
local function CreateServerSideUI(player)
    -- 检查用户名
    if player.Name ~= TARGET_USERNAME then
        return
    end

    -- 创建主UI
    local ScreenGui = Instance.new("ScreenGui")
    ScreenGui.Name = "TUPS9362 Gui v1"
    ScreenGui.Parent = player:WaitForChild("PlayerGui")
    ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

    -- 主窗口
    local MainFrame = Instance.new("Frame")
    MainFrame.Name = "MainFrame"
    MainFrame.Size = UDim2.new(0, 300, 0, 400)
    MainFrame.Position = UDim2.new(0.02, 0, 0.02, 0)
    MainFrame.BackgroundColor3 = Color3.fromRGB(10, 15, 30)
    MainFrame.BorderColor3 = Color3.fromRGB(0, 150, 255)
    MainFrame.BorderSizePixel = 2
    MainFrame.BackgroundTransparency = 0.1
    MainFrame.Parent = ScreenGui

    -- 添加发光效果
    local UIGradient = Instance.new("UIGradient")
    UIGradient.Color = ColorSequence.new({
        ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 100, 255)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 200, 255))
    })
    UIGradient.Rotation = 45
    UIGradient.Parent = MainFrame

    -- 标题栏
    local TitleBar = Instance.new("Frame")
    TitleBar.Name = "TitleBar"
    TitleBar.Size = UDim2.new(1, 0, 0, 25)
    TitleBar.BackgroundColor3 = Color3.fromRGB(0, 80, 160)
    TitleBar.BorderSizePixel = 0
    TitleBar.Parent = MainFrame

    local TitleGradient = Instance.new("UIGradient")
    TitleGradient.Color = ColorSequence.new({
        ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 100, 255)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 180, 255))
    })
    TitleGradient.Parent = TitleBar

    local TitleLabel = Instance.new("TextLabel")
    TitleLabel.Name = "TitleLabel"
    TitleLabel.Size = UDim2.new(1, 0, 1, 0)
    TitleLabel.BackgroundTransparency = 1
    TitleLabel.Text = "TUPS9362 Gui v1"
    TitleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    TitleLabel.TextSize = 16
    TitleLabel.Font = Enum.Font.GothamBold
    TitleLabel.TextStrokeTransparency = 0.8
    TitleLabel.Parent = TitleBar

    -- 滚动框架
    local ScrollFrame = Instance.new("ScrollingFrame")
    ScrollFrame.Name = "ScrollFrame"
    ScrollFrame.Size = UDim2.new(1, -5, 1, -30)
    ScrollFrame.Position = UDim2.new(0, 2, 0, 27)
    ScrollFrame.BackgroundColor3 = Color3.fromRGB(15, 20, 40)
    ScrollFrame.BackgroundTransparency = 0.2
    ScrollFrame.BorderSizePixel = 0
    ScrollFrame.ScrollBarThickness = 6
    ScrollFrame.ScrollBarImageColor3 = Color3.fromRGB(0, 150, 255)
    ScrollFrame.CanvasSize = UDim2.new(0, 0, 0, 1200)
    ScrollFrame.Parent = MainFrame

    -- 按钮容器
    local ButtonContainer = Instance.new("Frame")
    ButtonContainer.Name = "ButtonContainer"
    ButtonContainer.Size = UDim2.new(1, 0, 0, 1200)
    ButtonContainer.BackgroundTransparency = 1
    ButtonContainer.Parent = ScrollFrame

    -- 创建按钮的函数
    local function CreateButton(name, position, callback)
        local Button = Instance.new("TextButton")
        Button.Name = name
        Button.Size = UDim2.new(1, -10, 0, 35)
        Button.Position = position
        Button.BackgroundColor3 = Color3.fromRGB(30, 60, 120)
        Button.BorderColor3 = Color3.fromRGB(0, 180, 255)
        Button.Text = name
        Button.TextColor3 = Color3.fromRGB(255, 255, 255)
        Button.TextSize = 12
        Button.Font = Enum.Font.Gotham
        Button.Parent = ButtonContainer
        
        -- 添加按钮渐变效果
        local ButtonGradient = Instance.new("UIGradient")
        ButtonGradient.Color = ColorSequence.new({
            ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 120, 255)),
            ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 200, 255))
        })
        ButtonGradient.Parent = Button
        
        -- 鼠标悬停效果
        Button.MouseEnter:Connect(function()
            TweenService:Create(Button, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(40, 100, 200)}):Play()
        end)
        
        Button.MouseLeave:Connect(function()
            TweenService:Create(Button, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(30, 60, 120)}):Play()
        end)

        -- 服务器端处理点击事件
        Button.MouseButton1Click:Connect(function()
            callback()
        end)

        return Button
    end

    -- 功能按钮定义
    local buttonY = 5

    -- 所有玩家头顶图片功能
    CreateButton("所有玩家头顶图片", UDim2.new(0, 5, 0, buttonY), function()
        for _, targetPlayer in pairs(Players:GetPlayers()) do
            if targetPlayer.Character then
                local head = targetPlayer.Character:FindFirstChild("Head")
                if head then
                    local billboardGui = Instance.new("BillboardGui")
                    billboardGui.Adornee = head
                    billboardGui.Size = UDim2.new(15, 0, 15, 0)
                    billboardGui.StudsOffset = Vector3.new(0, 2, 0)
                    billboardGui.AlwaysOnTop = true
                    billboardGui.MaxDistance = 500
                    billboardGui.Parent = head

                    local imageLabel = Instance.new("ImageLabel")
                    imageLabel.Size = UDim2.new(1, 0, 1, 0)
                    imageLabel.BackgroundTransparency = 1
                    imageLabel.Image = "http://www.roblox.com/asset/?id=133396117662947"
                    imageLabel.ScaleType = Enum.ScaleType.Fit
                    imageLabel.Parent = billboardGui
                end
            end
        end
    end)

    buttonY = buttonY + 40

    -- 其他玩家头顶图片功能
    CreateButton("其他玩家头顶图片", UDim2.new(0, 5, 0, buttonY), function()
        for _, targetPlayer in pairs(Players:GetPlayers()) do
            if targetPlayer.Name ~= TARGET_USERNAME and targetPlayer.Character then
                local head = targetPlayer.Character:FindFirstChild("Head")
                if head then
                    local billboardGui = Instance.new("BillboardGui")
                    billboardGui.Adornee = head
                    billboardGui.Size = UDim2.new(15, 0, 15, 0)
                    billboardGui.StudsOffset = Vector3.new(0, 2, 0)
                    billboardGui.AlwaysOnTop = true
                    billboardGui.MaxDistance = 500
                    billboardGui.Parent = head

                    local imageLabel = Instance.new("ImageLabel")
                    imageLabel.Size = UDim2.new(1, 0, 1, 0)
                    imageLabel.BackgroundTransparency = 1
                    imageLabel.Image = "http://www.roblox.com/asset/?id=133396117662947"
                    imageLabel.ScaleType = Enum.ScaleType.Fit
                    imageLabel.Parent = billboardGui
                end
            end
        end
    end)

    buttonY = buttonY + 40

    -- 天空盒修改
    CreateButton("修改天空盒", UDim2.new(0, 5, 0, buttonY), function()
        if Lighting:FindFirstChild("Sky") then
            Lighting.Sky:Destroy()
        end

        local newSky = Instance.new("Sky")
        newSky.Name = "Sky"
        newSky.Parent = Lighting

        local imageId = "133396117662947"
        newSky.SkyboxBk = "http://www.roblox.com/asset/?id=" .. imageId
        newSky.SkyboxDn = "http://www.roblox.com/asset/?id=" .. imageId
        newSky.SkyboxFt = "http://www.roblox.com/asset/?id=" .. imageId
        newSky.SkyboxLf = "http://www.roblox.com/asset/?id=" .. imageId
        newSky.SkyboxRt = "http://www.roblox.com/asset/?id=" .. imageId
        newSky.SkyboxUp = "http://www.roblox.com/asset/?id=" .. imageId
    end)

    buttonY = buttonY + 40

    -- 播放音乐
    CreateButton("播放音乐", UDim2.new(0, 5, 0, buttonY), function()
        local sound = Instance.new("Sound")
        sound.SoundId = "rbxassetid://1839246711"
        sound.PlaybackSpeed = 0.8
        sound.Volume = 10
        sound.Looped = true
        sound.Parent = game.Workspace
        sound:Play()
    end)

    buttonY = buttonY + 40

    -- 粒子特效
    CreateButton("粒子特效", UDim2.new(0, 5, 0, buttonY), function()
        local particleTexture = "http://www.roblox.com/asset/?id=133396117662947"

        for _, player in pairs(Players:GetPlayers()) do
            local character = player.Character
            if character then
                for _, part in pairs(character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        local particleEmitter = Instance.new("ParticleEmitter")
                        particleEmitter.Texture = particleTexture
                        particleEmitter.Rate = 10
                        particleEmitter.Lifetime = NumberRange.new(1, 2)
                        particleEmitter.Parent = part
                    end
                end
            end
        end
    end)

    buttonY = buttonY + 40

    -- 所有物体贴图功能
    CreateButton("所有物体贴图", UDim2.new(0, 5, 0, buttonY), function()
        local decalTexture = "http://www.roblox.com/asset/?id=133396117662947"

        -- 改变所有物体的贴图
        local function applyDecalsToAllObjects()
            local descendants = game.Workspace:GetDescendants()
            for _, descendant in pairs(descendants) do
                if descendant:IsA("Part") or descendant:IsA("UnionOperation") then
                    -- 为每个面添加贴图
                    for _, face in pairs(Enum.NormalId:GetEnumItems()) do
                        local decal = Instance.new("Decal")
                        decal.Face = face
                        decal.Texture = decalTexture
                        decal.ZIndex = 99
                        decal.Parent = descendant
                    end
                end
            end
        end

        -- 执行贴图改变
        applyDecalsToAllObjects()

        -- 处理新创建的物体
        game.Workspace.DescendantAdded:Connect(function(descendant)
            wait(0.1) -- 稍等确保物体完全加载
            if descendant:IsA("Part") or descendant:IsA("UnionOperation") then
                -- 为新物体添加贴图
                for _, face in pairs(Enum.NormalId:GetEnumItems()) do
                    local decal = Instance.new("Decal")
                    decal.Face = face
                    decal.Texture = decalTexture
                    decal.ZIndex = 99
                    decal.Parent = descendant
                end
            end
        end)
    end)

    buttonY = buttonY + 40

    -- 跳杀画面（带音效版本）
    CreateButton("跳杀画面", UDim2.new(0, 5, 0, buttonY), function()
        for _, targetPlayer in ipairs(Players:GetPlayers()) do
            if targetPlayer:FindFirstChild("PlayerGui") then
                local ScreenGui = Instance.new("ScreenGui")
                local ImageLabel = Instance.new("ImageLabel")

                ScreenGui.Parent = targetPlayer.PlayerGui
                ScreenGui.IgnoreGuiInset = true
                ScreenGui.Name = "JumpScare"
                ScreenGui.ResetOnSpawn = false

                ImageLabel.Parent = ScreenGui
                ImageLabel.Size = UDim2.new(1, 0, 1, 0)
                ImageLabel.Image = "http://www.roblox.com/asset/?id=140662485725366"
                ImageLabel.BackgroundColor3 = Color3.new(0, 0, 0)
                ImageLabel.BackgroundTransparency = 0
                ImageLabel.ScaleType = Enum.ScaleType.Fit

                -- 添加音效到ScreenGui
                local sound = Instance.new("Sound", ScreenGui)
                sound.SoundId = "rbxassetid://6018028320"
                sound.Volume = 10
                sound.Looped = false  -- 不循环，只播放一次
                
                -- 监听音效结束事件
                sound.Ended:Connect(function()
                    if ScreenGui and ScreenGui.Parent then
                        ScreenGui:Destroy()
                    end
                end)
                
                -- 同时设置一个最大时间限制，防止音效过长
                delay(30, function()
                    if ScreenGui and ScreenGui.Parent then
                        ScreenGui:Destroy()
                    end
                end)
                
                -- 播放音效
                sound:Play()
            end
        end
    end)

    buttonY = buttonY + 40

    -- 彩虹效果
    CreateButton("彩虹效果", UDim2.new(0, 5, 0, buttonY), function()
        local colorCorrection = Instance.new("ColorCorrectionEffect")
        colorCorrection.Parent = Lighting
        colorCorrection.Enabled = true

        spawn(function()
            local counter = 0
            while true do
                counter = counter + 0.01
                if counter > 1 then counter = 0 end

                local hue = counter
                local color = Color3.fromHSV(hue, 1, 1)

                colorCorrection.TintColor = color
                Lighting.Ambient = Color3.fromHSV(hue, 0.3, 0.8)

                wait(0.1)
            end
        end)
    end)

    buttonY = buttonY + 40

    -- 完整的666恐怖效果
    CreateButton("666恐怖效果", UDim2.new(0, 5, 0, buttonY), function()
        local function Apply666Effects(object)
            for _, child in pairs(object:GetChildren()) do
                if child:IsA("BasePart") then
                    -- 将部件变为黑色
                    child.BrickColor = BrickColor.new("Really black")
                    child.TopSurface = Enum.SurfaceType.Smooth
                    child.BottomSurface = Enum.SurfaceType.Smooth

                    -- 添加红色选择框
                    local selectionBox = Instance.new("SelectionBox")
                    selectionBox.Adornee = child
                    selectionBox.Color = BrickColor.new("Really red")
                    selectionBox.Parent = child

                    -- 添加红色点光源
                    local pointLight = Instance.new("PointLight")
                    pointLight.Color = Color3.new(1, 0, 0)
                    pointLight.Range = 15
                    pointLight.Brightness = 5
                    pointLight.Parent = child

                    -- 添加火焰效果
                    local fire = Instance.new("Fire")
                    fire.Size = 19
                    fire.Heat = 22
                    fire.Parent = child

                    -- 创建BillboardGui和TextLabel
                    local billboard = Instance.new("BillboardGui")
                    billboard.Name = "stuf"
                    billboard.Adornee = child
                    billboard.Size = UDim2.new(2.5, 0, 2.5, 0)
                    billboard.Parent = child

                    local textLabel = Instance.new("TextLabel")
                    textLabel.Text = "666 666 666 666 666 666"
                    textLabel.Font = Enum.Font.SourceSansBold
                    textLabel.TextSize = 48
                    textLabel.TextColor3 = Color3.new(1, 0, 0)
                    textLabel.Size = UDim2.new(1, 0, 1.25, 0)
                    textLabel.Position = UDim2.new(0, -22, -0.1, 0)
                    textLabel.BackgroundTransparency = 1
                    textLabel.Parent = billboard
                end

                -- 递归处理子对象
                if #child:GetChildren() > 0 then
                    Apply666Effects(child)
                end
            end
        end

        -- 对整个工作空间应用效果
        Apply666Effects(game.Workspace)

        -- 对工作空间的直接子对象也应用效果
        for _, obj in pairs(workspace:GetChildren()) do
            if obj:IsA("BasePart") then
                local billboard = Instance.new("BillboardGui")
                billboard.Name = "stuf"
                billboard.Adornee = obj
                billboard.Size = UDim2.new(2.5, 0, 2.5, 0)
                billboard.Parent = obj

                local textLabel = Instance.new("TextLabel")
                textLabel.Text = "666 666 666 666 666 666"
                textLabel.Font = Enum.Font.SourceSansBold
                textLabel.TextSize = 48
                textLabel.TextColor3 = Color3.new(1, 0, 0)
                textLabel.Size = UDim2.new(1, 0, 1.25, 0)
                textLabel.Position = UDim2.new(0, -22, -0.1, 0)
                textLabel.BackgroundTransparency = 1
                textLabel.Parent = billboard
            end
        end
    end)

    buttonY = buttonY + 40

    -- 显示提示文字
    CreateButton("显示提示", UDim2.new(0, 5, 0, buttonY), function()
        local hint = Instance.new("Hint", game.Workspace)
        hint.Text = "Team TUPS9362 Join Today!"

        spawn(function()
            wait(10)
            hint:Destroy()
        end)
    end)

    buttonY = buttonY + 40

    -- 一键清除所有效果
    CreateButton("清除所有效果", UDim2.new(0, 5, 0, buttonY), function()
        -- 清除天空盒
        if Lighting:FindFirstChild("Sky") then
            Lighting.Sky:Destroy()
        end

        -- 清除声音
        for _, sound in pairs(game.Workspace:GetChildren()) do
            if sound:IsA("Sound") then
                sound:Stop()
                sound:Destroy()
            end
        end

        -- 清除粒子效果
        for _, obj in pairs(workspace:GetDescendants()) do
            if obj:IsA("ParticleEmitter") then
                obj:Destroy()
            end
        end

        -- 清除BillboardGui
        for _, obj in pairs(workspace:GetDescendants()) do
            if obj:IsA("BillboardGui") then
                obj:Destroy()
            end
        end

        -- 清除选择框
        for _, obj in pairs(workspace:GetDescendants()) do
            if obj:IsA("SelectionBox") then
                obj:Destroy()
            end
        end

        -- 清除点光源
        for _, obj in pairs(workspace:GetDescendants()) do
            if obj:IsA("PointLight") then
                obj:Destroy()
            end
        end

        -- 清除火焰效果
        for _, obj in pairs(workspace:GetDescendants()) do
            if obj:IsA("Fire") then
                obj:Destroy()
            end
        end

        -- 清除屏幕GUI
        for _, targetPlayer in pairs(Players:GetPlayers()) do
            if targetPlayer:FindFirstChild("PlayerGui") then
                for _, gui in pairs(targetPlayer.PlayerGui:GetChildren()) do
                    if gui.Name == "JumpScare" then
                        gui:Destroy()
                    end
                end
            end
        end

        -- 清除提示
        for _, hint in pairs(game.Workspace:GetChildren()) do
            if hint:IsA("Hint") then
                hint:Destroy()
            end
        end

        -- 清除贴图
        for _, obj in pairs(workspace:GetDescendants()) do
            if obj:IsA("Decal") then
                obj:Destroy()
            end
        end

        -- 恢复光照设置
        Lighting.Ambient = Color3.new(0.5, 0.5, 0.5)

        -- 清除颜色校正效果
        for _, effect in pairs(Lighting:GetChildren()) do
            if effect:IsA("ColorCorrectionEffect") then
                effect:Destroy()
            end
        end

        -- 恢复部件颜色
        for _, obj in pairs(workspace:GetDescendants()) do
            if obj:IsA("BasePart") then
                obj.BrickColor = BrickColor.new("Medium stone grey")
                obj.TopSurface = Enum.SurfaceType.Studs
                obj.BottomSurface = Enum.SurfaceType.Inlet
            end
        end
    end)

    -- 修复的窗口拖动功能
    local dragging = false
    local dragStart
    local startPos

    TitleBar.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            dragStart = input.Position
            startPos = MainFrame.Position
        end
    end)

    TitleBar.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = false
        end
    end)

    TitleBar.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement and dragging then
            local delta = input.Position - dragStart
            MainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
        end
    end)

    -- 关闭按钮
    local CloseButton = Instance.new("TextButton")
    CloseButton.Name = "CloseButton"
    CloseButton.Size = UDim2.new(0, 20, 0, 20)
    CloseButton.Position = UDim2.new(1, -25, 0, 2)
    CloseButton.BackgroundColor3 = Color3.fromRGB(255, 80, 80)
    CloseButton.BorderSizePixel = 0
    CloseButton.Text = "X"
    CloseButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    CloseButton.TextSize = 14
    CloseButton.Font = Enum.Font.GothamBold
    CloseButton.Parent = TitleBar

    -- 关闭按钮渐变
    local CloseButtonGradient = Instance.new("UIGradient")
    CloseButtonGradient.Color = ColorSequence.new({
        ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 100, 100)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(200, 50, 50))
    })
    CloseButtonGradient.Parent = CloseButton

    CloseButton.MouseButton1Click:Connect(function()
        ScreenGui:Destroy()
    end)
end

-- 为所有现有玩家创建UI（但只对特定用户名创建）
for _, player in pairs(Players:GetPlayers()) do
    CreateServerSideUI(player)
end

-- 为新加入的玩家创建UI
Players.PlayerAdded:Connect(CreateServerSideUI)